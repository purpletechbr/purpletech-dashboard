<% if(page == '/dashboard') { %>
    <% if(user.admin) { %>
        <%- include content/dashboardAdmin.ejs %>
    <% } else { %>
        <%- include content/dashboardUser.ejs %>
    <% } %>
<% } else if(page == '/profile') { %>
    <%- include content/profile.ejs %>
<% } else if(page == '/project') { %>
    <%- include content/project.ejs %>
<% } else if(page == '/allproject') { %>
    <%- include content/allProject.ejs %>
<% } else if(page == '/users') { %>
    <%- include content/users.ejs %>
<% } %>